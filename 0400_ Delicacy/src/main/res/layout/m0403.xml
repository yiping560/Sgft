<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back03"
    android:orientation="vertical"
    android:padding="@dimen/p10"
    tools:context=".M0403">

    <TextView
        android:id="@+id/m0403_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint0403_t001"
        android:text="@string/m0403_t001"
        android:textColorHint="@color/Silver"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/m0403_b001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p50"
        android:text="@string/m0403_b001"
        android:textSize="@dimen/fontsize" />

    <Button
        android:id="@+id/m0403_b002"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p50"
        android:text="@string/m0403_b002"
        android:textSize="@dimen/fontsize" />

    <Button
        android:id="@+id/m0403_b003"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p50"
        android:text="@string/m0403_b003"
        android:textSize="@dimen/fontsize" />
</LinearLayout>