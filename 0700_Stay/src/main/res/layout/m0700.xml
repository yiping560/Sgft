<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back04"
    android:orientation="vertical"
    android:padding="@dimen/p10"
    tools:context=".M0700">

    <Button
        android:id="@+id/m0700_b004"
        android:layout_width="@dimen/p100"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/White"
        android:gravity="center"
        android:text="@string/m0700_b004"
        android:textColor="@color/black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="85dp"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="52dp" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/m0700_t001"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/p10"
            android:background="@color/White"
            android:gravity="center"
            android:minWidth="88dp"
            android:minHeight="44dp"
            android:text="@string/m0700_t001"
            android:textColor="@color/Teal"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            tools:layout_conversion_absoluteHeight="32dp"
            tools:layout_conversion_absoluteWidth="180dp"
            tools:layout_editor_absoluteX="115dp"
            tools:layout_editor_absoluteY="20dp" />
    </TableRow>

    <ProgressBar
        android:id="@+id/progressBar"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="30" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10">

        <Button
            android:id="@+id/m0700_b001"
            style="?attr/buttonBarButtonStyle"
            android:layout_width="@dimen/p100"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/p50"
            android:background="@android:drawable/btn_default"
            android:text="@string/m0700_b001"
            android:textColor="@color/teal_700"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            tools:ignore="TextContrastCheck"
            tools:layout_conversion_absoluteHeight="48dp"
            tools:layout_conversion_absoluteWidth="85dp"
            tools:layout_editor_absoluteX="10dp"
            tools:layout_editor_absoluteY="52dp" />

        <Button
            android:id="@+id/m0700_b002"
            style="?attr/buttonBarButtonStyle"
            android:layout_width="@dimen/p100"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/p100"
            android:background="@android:drawable/btn_default"
            android:gravity="center"
            android:text="@string/m0700_b002"
            android:textColor="@color/Teal"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            tools:ignore="TextContrastCheck"
            tools:layout_conversion_absoluteHeight="48dp"
            tools:layout_conversion_absoluteWidth="85dp"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="120dp" />

    </TableRow>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/p180"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:srcCompat="@drawable/temp_map" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="@dimen/p85"
        android:layout_marginTop="@dimen/p10">

        <Button
            android:id="@+id/m0700_b005"
            style="?attr/buttonBarButtonStyle"
            android:layout_width="@dimen/p170"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginStart="@dimen/p30"
            android:layout_marginTop="@dimen/p10"
            android:background="@android:drawable/btn_default"
            android:paddingTop="@dimen/p10"
            android:text="@string/m0700_b005"
            android:textColor="@color/Teal"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            tools:layout_conversion_absoluteHeight="48dp"
            tools:layout_conversion_absoluteWidth="85dp"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="120dp" />

        <Button
            android:id="@+id/m0700_b003"
            style="?attr/buttonBarButtonStyle"
            android:layout_width="@dimen/p170"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginTop="@dimen/p10"
            android:background="@android:drawable/btn_default"
            android:paddingTop="@dimen/p10"
            android:text="@string/m0700_b003"
            android:textColor="@color/Teal"
            android:textSize="@dimen/fontsize"
            android:textStyle="bold"
            tools:layout_conversion_absoluteHeight="48dp"
            tools:layout_conversion_absoluteWidth="85dp"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="120dp" />
    </TableRow>

</LinearLayout>