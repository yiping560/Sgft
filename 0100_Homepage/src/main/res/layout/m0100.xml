            android:text="@string/m0100_b005"
            android:textColor="@color/Red"
            android:textSize="@dimen/fontsize" />

        <Button
            android:id="@+id/m0100_b006"
            android:layout_width="@dimen/p350"
            android:layout_height="wrap_content"
            android:layout_below="@+id/m0100_b005"
