            android:text="@string/m0100_b003"
            android:textColor="@color/Red"
            android:textSize="@dimen/fontsize" />

        <Button
            android:id="@+id/m0100_b004"
            android:layout_width="@dimen/p350"
            android:layout_height="wrap_content"
            android:layout_below="@+id/m0100_b002"
            android:layout_alignEnd="@+id/m0100_b002"
            android:layout_marginStart="@dimen/p10"
            android:layout_marginTop="@dimen/p20"
            android:layout_marginEnd="-2dp"
