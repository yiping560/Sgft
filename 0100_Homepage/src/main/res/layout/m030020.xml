            android:text="@string/m030020_add"
            android:textSize="@dimen/fontsize" />

        <Button
            android:id="@+id/m030020_b011"
            android:layout_width="@dimen/p50"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/Margins"
            android:text="@string/m030020_delete"
            android:textSize="@dimen/fontsize"
            tools:ignore="TextContrastCheck" />

    </TableRow>

    <TextView
        android:id="@+id/m030040_myname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="end"
        android:text="@string/myname_13"
        android:textSize="@dimen/font21" />

</LinearLayout>