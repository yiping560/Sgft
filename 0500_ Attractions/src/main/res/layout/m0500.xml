<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m0500_r001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back04"
    android:orientation="vertical"
    android:padding="@dimen/p10"
    tools:context=".M0500">

    <LinearLayout
        android:id="@+id/l1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/p30">

            <TextView
                android:id="@+id/m0500_t001"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/teal_200"
                android:gravity="center"
                android:text="@string/m0500_t001"
                android:textSize="@dimen/fontsize" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/p50">

            <Spinner
                android:id="@+id/m0500_s001"
                android:layout_width="@dimen/p200"
                android:layout_height="match_parent"
                android:entries="@array/county"
                android:spinnerMode="dialog"
                android:textSize="@dimen/fontsize"
                tools:ignore="SpeakableTextPresentCheck" />

            <Spinner
                android:id="@+id/m0500_s002"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:entries="@array/area"
                android:textSize="@dimen/fontsize"
                tools:ignore="SpeakableTextPresentCheck" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/p50"
            android:layout_marginTop="@dimen/p30">

            <Button
                android:id="@+id/m0500_b002"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@android:drawable/ic_menu_search"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/m0500_t005"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/teal_700"
                android:gravity="center"
                android:text="@string/m0500_t005"
                android:textSize="@dimen/fontsize"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/p10"
            android:gravity="center">

            <Button
                android:id="@+id/m0500_b001"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/m0500_b001"
                android:textSize="@dimen/fontsize" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/p30"
            android:layout_marginTop="@dimen/p20"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/m0500_v001"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/design_default_color_primary"
                app:srcCompat="@drawable/img11" />

            <TextView
                android:id="@+id/m0500_t008"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/purple_200"
                android:text="@string/m0500_t008"
                android:textSize="@dimen/fontsize" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/p30"
            android:layout_marginTop="@dimen/p30">

            <ImageView
                android:id="@+id/m0500_i002"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/design_default_color_primary"
                app:srcCompat="@drawable/imag01" />

            <TextView
                android:id="@+id/m0500_t009"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/purple_200"
                android:text="@string/m0500_t009"
                android:textSize="@dimen/fontsize" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/p30"
            android:layout_marginTop="@dimen/p30">

            <ImageView
                android:id="@+id/m0500_v003"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/design_default_color_primary"
                app:srcCompat="@drawable/imag06" />

            <TextView
                android:id="@+id/m0500_t010"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/purple_200"
                android:text="@string/m0500_t010"
                android:textSize="@dimen/fontsize" />
        </TableRow>

        <ImageView
            android:id="@+id/m0500_logo"
            android:layout_width="@dimen/p150"
            android:layout_height="@dimen/p150"
            android:layout_marginTop="@dimen/p170"
            app:srcCompat="@drawable/logo46" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="@dimen/p10"
        android:layout_marginBottom="@dimen/p10"
        android:text="@string/myname15"
        android:textSize="@dimen/fontsize" />

    <LinearLayout
        android:id="@+id/l2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/back03"
        android:padding="@dimen/p10"
        android:visibility="invisible">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="@dimen/p200"
            android:layout_height="@dimen/p100"
            android:layout_weight="1"
            tools:srcCompat="@drawable/imag01" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/p10"
            android:layout_weight="1"
            android:background="@color/Aqua"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/m0501_t001a"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/p10"
                android:text="@string/m0501_t001a"
                android:textSize="@dimen/fontsize16" />

            <TextView
                android:id="@+id/m0501_t001b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/p10"
                android:text="@string/m0501_t001b"
                android:textSize="@dimen/fontsize16" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>